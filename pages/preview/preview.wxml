<!--preview.wxml-->
<import src="../../components/xincell/xincell.wxml" />
<import src="../../components/TopTip/TopTip.wxml" />
<view class="preview_container">
  <template is="TopTip" data="{{...toptip_data}}"></template>
  <view class="preview_head">
    <template is="xincell" data="{{postcard}}" />
  </view>
  <button class="preview_add_collect" type="primary" bindtap="onSend">发送</button>
  <button class="preview_add_collect" bindtap="onEdit">返回编辑</button>
</view>