
.topnav_container {
  display: flex;
  width: 100%;
  height: 64px;
  justify-content: space-around;
  padding: 20px 0px;
}

.nav_item {
  display: flex;
  flex-direction: column;
  width: 64px;
  height: 64px;
  margin: auto;
  justify-content: center;
  text-align: center;
}

.nav_image {
  margin: 5px auto;
  width: 33px;
  height: 33px;
}

.nav_title {
  width: 100%;
  text-align: center;
  font-size: 12px;
}